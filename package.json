		"vitest": "^0.29.0"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.8.7",
		"@supabase/supabase-js": "^2.7.1",
		"@types/marked": "^4.0.8",
		"highlight.js": "^11.7.0",
		"marked": "^4.2.12",
		"supabase": "^1.36.6"
	}
}
